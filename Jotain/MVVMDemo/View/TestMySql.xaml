        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVMDemo.View"
        mc:Ignorable="d"
        Title="TestMySql" Height="300" Width="300">
    <Grid>
        <Grid HorizontalAlignment="Left" Height="230" Margin="27,10,0,0" VerticalAlignment="Top" Width="255"/>
        <DockPanel>
            <Button x:Name="btnGetData" Content="Hae oppilaat Mysql:sta"
                   Click="btnGetData_Click" DockPanel.Dock="Top" />
            <DataGrid x:Name="dgStudents" ItemsSource="{Binding}"/>
        </DockPanel>
    </Grid>
</Window>
